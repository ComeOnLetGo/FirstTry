	<xs:complexType name="ProjectFile">
		<xs:complexContent>
			<xs:extension base="GenericItem">
				<xs:sequence>
					<xs:element name="SpecificTargetLanguages">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="SpecificTargetLanguage" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="LanguageFiles">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="LanguageFile" type="LanguageFile" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="PreviousBilingualFiles">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="PreviousBilingualFile" type="PreviousBilingualFile" minOccurs="0" maxOccurs="unbounded"/>
