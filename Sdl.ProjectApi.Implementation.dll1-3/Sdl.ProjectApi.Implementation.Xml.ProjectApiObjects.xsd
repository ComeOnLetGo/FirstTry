			<xs:extension base="GenericItem">
				<xs:sequence>
					<xs:element name="TaskTemplateIds">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="TaskTemplateId" type="xs:string" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Files">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="TaskFile" type="TaskFile" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
