						</xs:complexType>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="ParentProjectFileGuid" type="xs:string" use="optional"/>
				<xs:attribute name="FilterDefinitionId" type="xs:string" use="optional"/>
